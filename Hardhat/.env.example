# Network Configuration
# Get your API key from: https://www.alchemy.com/
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Deployer Account
# Your wallet private key (without 0x prefix)
# WARNING: Never commit your actual private key to version control
DEPLOYER_PRIVATE_KEY=your_deployer_private_key_here

# Contract Verification
# Get your API key from: https://basescan.org/myapikey
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Optional: Mainnet Forking (for local testing)
# Set to "true" to enable mainnet forking in local Hardhat node
MAINNET_FORKING_ENABLED=false
